{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://wordpress.com/wp-login.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_login"
      },
      "text": "Test"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//body"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_pass"
      },
      "text": "1234"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//body"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "wp-submit"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Account Suspended!If you believe your account is suspended in error please contact us"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}