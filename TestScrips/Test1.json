{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://wordpress.com/wp-login.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_login"
      },
      "text": "Abcd"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_pass"
      },
      "text": "Test"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "wp-submit"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "ERROR: The password you entered for the email or username abcd is incorrect. Lost your password?"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 10
}